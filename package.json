{
  "name": "clavistry",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "homepage": "https://eschiebel.github.io/Clavistry",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "check": "tsc --noEmit && biome check .",
    "format": "biome format .",
    "predeploy": "npm run check && npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "gh-pages": "^6.3.0",
    "typescript": "^5.6.2",
    "vite": "^7.1.7"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@11.6.0"
}
